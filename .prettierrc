{
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "proseWrap": "always"
}
